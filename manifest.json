{
  "manifest_version": 3,
  "name": "ChatGPT JumpBack",
  "version": "0.6.1",
  "description": "Quickly jump back to your last reading position in ChatGPT.",
  "permissions": ["contextMenus", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": {
      "16": "icon_gray_16x16.png",
      "48": "icon_gray_48x48.png",
      "128": "icon_gray_128x128.png"
    }
  },
  "icons": {
    "16": "icon_16x16.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  }
}
