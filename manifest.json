{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.0.2",
  "version_name": "1.0.2",
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "permissions": ["contextMenus", "tabs", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
  "action": {
    "default_icon": {
      "16": "gray_16.png",
      "48": "gray_48.png",
      "128": "gray_128.png"
    },
    "default_title": "__MSG_actionTitle__"
  },
  "icons": {
    "16": "color_16.png",
    "48": "color_48.png",
    "128": "color_128.png"
  },
  "homepage_url": "https://chromewebstore.google.com/detail/chatgpt-jumpback/aadmenichpgbodjbkpenadnmngpanloh",
  "options_page": "options.html",
  "commands": {
    "jump-back": {
      "description": "Jump back to last reading position",
      "suggested_key": {
        "default": "Alt+J",
        "mac": "Alt+Shift+J"
      }
    }
  }
}
